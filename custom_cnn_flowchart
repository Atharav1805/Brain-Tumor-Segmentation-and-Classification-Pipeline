digraph Custom_CNN {
	rankdir=LR size="10,5"
	A [label="Input
256×256×1"]
	B [label="Conv Block 1
Conv 3×3 → ReLU
MaxPool 2×2 (32 filters)"]
	C [label="Conv Block 2
Conv 3×3 → ReLU
MaxPool 2×2 (64 filters)"]
	D [label="Conv Block 3
Conv 3×3 → ReLU
MaxPool 2×2 (128 filters)"]
	E [label=Flatten]
	F [label="Dense 512 + ReLU
Dropout 0.5"]
	G [label="Output Layer
Softmax (3 classes)"]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
	F -> G
}
